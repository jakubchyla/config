{
    "workbench.colorTheme": "Material Theme",
    "workbench.iconTheme": "material-icon-theme",
    "C_Cpp.updateChannel": "Insiders",
    "terminal.integrated.fontFamily": "Source Code Pro",
    "terminal.integrated.rendererType": "canvas",
    "window.zoomLevel": 1,
